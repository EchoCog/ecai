# Client public env vars
# Disable telemetry collection for analytics
NEXT_PUBLIC_DISABLE_TELEMETRY=false
# Environment name for client-side configuration
NEXT_PUBLIC_ENV_NAME=local
# Logging level for client-side debugging
NEXT_PUBLIC_LOG_LEVEL=debug
# PostHog analytics host URL
NEXT_PUBLIC_POSTHOG_HOST=https://us.i.posthog.com
# PostHog project API key
NEXT_PUBLIC_POSTHOG_KEY=phc_C1L7Idffoa2oRT54XZDGh4GSdOPFgzliJiusFnVV8Wz
# Tunnel URL for local development
NEXT_PUBLIC_TUNNEL_FORWARDING_URL=https://your-tunnel-url.ngrok-free.app

# General
# Base URL for the application
BASE_URL=http://localhost:3000
# 32-byte base64 encoded encryption key for data security
ENCRYPTION_KEY=your-32-byte-base64-encoded-encryption-key
# License tier (free/paid)
LICENSE_KEY=free
# Auth token for ngrok tunneling service
NGROK_AUTHTOKEN=
# Starter template type
STARTER=next
# Storage backend type (FILE_SYSTEM/DATABASE)
STORAGE_TYPE=FILE_SYSTEM
# Vite API base URL for frontend
VITE_API_BASE_URL=http://localhost:3000
# Vite base URL for frontend
VITE_BASE_URL=http://localhost:3000

# Available providers: AmazonBedrock, Anthropic, Cohere, Deepseek, Github, Google, Groq, HuggingFace, Hyperbolic, LMStudio, Mistral, Ollama, OpenAI, OpenAILike, OpenRouter, Perplexity, Together, xAI
# Default LLM provider for AI operations
DEFAULT_LLM_PROVIDER=Anthropic
# Default AI model to use
DEFAULT_LLM_MODEL=claude-4-sonnet-20250514

# Get your Anthropic API Key in your account settings - https://console.anthropic.com/settings/keys
ANTHROPIC_API_KEY= # API key for Anthropic Claude models

# API key for OpenRouter service (optional)
# OPEN_ROUTER_API_KEY=

# Base URL for local Ollama instance (optional)
# OLLAMA_API_BASE_URL=http://localhost:11434

# PostgreSQL database connection string
DATABASE_URL=postgresql://username:password@localhost:5432/database_name
# Secret key for authentication/session management
AUTH_SECRET=your-44-character-auth-secret-string-here

# Netlify API token for deployment automation
NETLIFY_AUTH_TOKEN=
